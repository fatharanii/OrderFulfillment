<?xml version="1.0" encoding="UTF-8"?>
<specificationSet xmlns="http://www.yawlfoundation.org/yawlschema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="4.0" xsi:schemaLocation="http://www.yawlfoundation.org/yawlschema http://www.yawlfoundation.org/yawlschema/YAWL_Schema4.0.xsd">
  <specification uri="orderfulfillment">
    <name>Order Fulfillment</name>
    <documentation>Order Fulfillment Workflow Model</documentation>
    <metaData>
      <title>Order Fulfillment</title>
      <creator>RIVER</creator>
      <description>Order Fulfillment Workflow Model</description>
      <coverage>4.3.1.772</coverage>
      <version>0.15</version>
      <persistent>false</persistent>
      <identifier>UID_30bf48ca-d609-4e8c-abdb-2bf48cf6a6e5</identifier>
    </metaData>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
      <xs:complexType name="PurchaseOrderType">
        <xs:sequence>
          <xs:element name="Company" type="CompanyType" />
          <xs:element name="Order" type="OrderType" />
          <xs:element name="FreightCost" type="xs:double" />
          <xs:element name="DeliveryLocation" type="xs:string" />
          <xs:element name="InvoiceRequired" type="xs:boolean" />
          <xs:element name="PrePaid" type="xs:boolean" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CompanyType">
        <xs:sequence>
          <xs:element name="Name" type="xs:string" />
          <xs:element name="Address" type="xs:string" />
          <xs:element name="City" type="xs:string" />
          <xs:element name="State" type="xs:string" />
          <xs:element name="PostCode" type="xs:string" />
          <xs:element name="Phone" type="xs:string" />
          <xs:element name="Fax" type="xs:string" />
          <xs:element name="BusinessNumber" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="OrderType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="OrderDate" type="xs:date" />
          <xs:element name="Currency" type="CurrencyType" />
          <xs:element name="OrderTerms" type="xs:string" />
          <xs:element name="RevisionNumber" type="ZeroType" />
          <xs:element name="Remarks" type="xs:string" />
          <xs:element name="OrderLines" type="OrderLinesType" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="OrderApprovalType">
        <xs:sequence>
          <xs:element name="OrderType" type="OrderType" />
          <xs:element name="Approved" type="xs:boolean" />
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="CurrencyType">
        <xs:restriction base="xs:string">
          <xs:enumeration value="AUD" />
          <xs:enumeration value="USD" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="ZeroType">
        <xs:restriction base="xs:integer">
          <xs:minInclusive value="0" />
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="OrderLinesType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" name="Line" type="LineType" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="LineType">
        <xs:sequence>
          <xs:element name="LineNumber" type="OneType" />
          <xs:element name="UnitCode" type="xs:string" />
          <xs:element name="UnitDescription" type="xs:string" />
          <xs:element name="UnitQuantity" type="OneType" />
          <xs:element name="Action" type="ActionType" />
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="ActionType">
        <xs:restriction base="xs:string">
          <xs:enumeration value="" />
          <xs:enumeration value="Added" />
          <xs:enumeration value="Modified" />
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="RouteGuideType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="DeliveryLocation" type="xs:string" />
          <xs:element name="Trackpoints" type="TrackpointsType" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="TrackpointsType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" name="Trackpoint" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="TrailerUsageType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="Packages" type="PackagesType" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="PackagesType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" name="Package" type="PackageType" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="PackageType">
        <xs:sequence>
          <xs:element name="PackageID" type="xs:string" />
          <xs:element name="Volume" type="PackageVolume" />
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="PackageVolume">
        <xs:restriction base="xs:integer">
          <xs:enumeration value="25" />
          <xs:enumeration value="50" />
          <xs:enumeration value="100" />
          <xs:enumeration value="200" />
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="TransportationQuoteType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="NumberOfPackages" type="OneType" />
          <xs:element name="TotalVolume" type="xs:integer" />
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="ShipmentCost" type="xs:double" />
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="OneType">
        <xs:restriction base="xs:integer">
          <xs:minInclusive value="1" />
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="PickupInstructionsType">
        <xs:sequence>
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="PickupDate" type="xs:date" />
          <xs:element name="PickupInstructions" type="xs:string" />
          <xs:element name="PickupSpot" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="DeliveryInstructionsType">
        <xs:sequence>
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="DeliveryDate" type="xs:date" />
          <xs:element name="DeliveryInstructions" type="xs:string" />
          <xs:element name="DeliveryLocation" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ShipmentInformationDocumentType">
        <xs:sequence>
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="NumberOfPackages" type="OneType" />
          <xs:element name="Truckload" type="xs:boolean" />
          <xs:element name="AuthorizationCode" type="xs:string" />
          <xs:element name="ConsigneeNumber" type="xs:integer" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="BillOfLadingType">
        <xs:sequence>
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="NumberOfPackages" type="OneType" />
          <xs:element name="Truckload" type="xs:boolean" />
          <xs:element name="AuthorizationCode" type="xs:string" />
          <xs:element name="ConsigneeNumber" type="xs:integer" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CarrierManifestType">
        <xs:sequence>
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="OrderNumber" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ShipmentNoticeType">
        <xs:sequence>
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="NumberOfPackages" type="OneType" />
          <xs:element name="Truckload" type="xs:boolean" />
          <xs:element name="PickupInstructions" type="PickupInstructionsType" />
          <xs:element name="StartLoad" type="xs:time" />
          <xs:element name="EndLoad" type="xs:time" />
          <xs:element name="DeliveryInstructions" type="DeliveryInstructionsType" />
          <xs:element name="ClaimsDeadline" type="xs:duration" />
          <xs:element name="DriverNumber" type="xs:string" />
          <xs:element name="DriverName" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ShipmentInvoiceType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="Company" type="CompanyType" />
          <xs:element name="ShipmentCost" type="xs:double" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="FreightInvoiceType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="Company" type="CompanyType" />
          <xs:element name="FreightCost" type="xs:double" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ShipmentPaymentOrderType">
        <xs:sequence>
          <xs:element name="ShipmentPaymentOrderNumber" type="xs:string" />
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="ShipmentCost" type="xs:double" />
          <xs:element name="Beneficiary" type="BeneficiaryType" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ShipmentPaymentOrderApprovalType">
        <xs:sequence>
          <xs:element name="Approved" type="xs:boolean" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ShipmentPaymentType">
        <xs:sequence>
          <xs:element name="Balance" type="xs:double" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="BeneficiaryType">
        <xs:sequence>
          <xs:element name="Beneficiary" type="CompanyType" />
          <xs:element name="Bank" type="xs:string" />
          <xs:element name="BankCode" type="xs:string" />
          <xs:element name="AccountName" type="xs:string" />
          <xs:element name="AccountNumber" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="DebitAdjustmentType">
        <xs:sequence>
          <xs:element name="StillDue" type="xs:double" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CreditAdjustmentType">
        <xs:sequence>
          <xs:element name="Reimbursement" type="xs:double" />
          <xs:element name="Beneficiary" type="BeneficiaryType" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ShipmentRemittanceAdviceType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="AmountDebited" type="xs:double" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="TrackpointNoticeType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="Trackpoint" type="xs:string" />
          <xs:element name="ArrivalTime" type="xs:time" />
          <xs:element name="DepartureTime" type="xs:time" />
          <xs:element name="Notes" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="TrackpointNoticesType">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="TrackpointNotice" type="TrackpointNoticeType" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="TrackpointOrderEntryType">
        <xs:sequence>
          <xs:element name="TrackpointNotice" type="TrackpointNoticeType" />
          <xs:element name="Report" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="TrackpointOrderEntriesType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" name="TrackpointOrderEntry" type="TrackpointOrderEntryType" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ShipmentStatusInquiryType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="Inquiry" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="AcceptanceCertificateType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="AcceptanceDate" type="xs:date" />
          <xs:element name="DeliveryNotes" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ReturnMerchandiseType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="AcceptanceDate" type="xs:date" />
          <xs:element name="CheckedOrderLines" type="CheckedOrderLinesType" />
          <xs:element name="ReasonForReturn" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="LossOrDamageClaimType">
        <xs:sequence>
          <xs:element name="OrderNumber" type="xs:string" />
          <xs:element name="ShipmentNumber" type="xs:string" />
          <xs:element name="AcceptanceDate" type="xs:date" />
          <xs:element name="ReasonForClaim" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CheckedOrderLinesType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" name="Line" type="LineType" />
          <xs:element name="Selected" type="xs:boolean" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ReturnMerchandiseApprovalType">
        <xs:sequence>
          <xs:element name="ReturnMerchandise" type="ReturnMerchandiseType" />
          <xs:element name="Approved" type="xs:boolean" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="LossOrDamageClaimApprovalType">
        <xs:sequence>
          <xs:element name="LossOrDamageClaim" type="LossOrDamageClaimType" />
          <xs:element name="Approved" type="xs:boolean" />
        </xs:sequence>
      </xs:complexType>
    </xs:schema>
    <decomposition id="Overall" isRootNet="true" xsi:type="NetFactsType">
      <localVariable>
        <index>0</index>
        <name>AcceptanceCertificate</name>
        <type>AcceptanceCertificateType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>&lt;OrderNumber/&gt; &lt;ShipmentNumber/&gt; &lt;AcceptanceDate&gt;2010-06-02&lt;/AcceptanceDate&gt; &lt;DeliveryNotes/&gt;</initialValue>
      </localVariable>
      <localVariable>
        <index>1</index>
        <name>ShipmentNotice</name>
        <type>ShipmentNoticeType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>2</index>
        <name>POrder</name>
        <type>PurchaseOrderType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>3</index>
        <name>PO_timedout</name>
        <type>boolean</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>false</initialValue>
      </localVariable>
      <localVariable>
        <index>4</index>
        <name>SP_timedout</name>
        <type>boolean</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>false</initialValue>
      </localVariable>
      <localVariable>
        <index>5</index>
        <name>POApproval</name>
        <type>boolean</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue>false</initialValue>
      </localVariable>
      <localVariable>
        <index>6</index>
        <name>TransportationQuote</name>
        <type>TransportationQuoteType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <processControlElements>
        <inputCondition id="InputCondition">
          <flowsInto>
            <nextElementRef id="Ordering" />
          </flowsInto>
        </inputCondition>
        <task id="Ordering">
          <name>Ordering</name>
          <flowsInto>
            <nextElementRef id="OutputCondition" />
            <isDefaultFlow />
          </flowsInto>
          <flowsInto>
            <nextElementRef id="Carrier_Appointment" />
            <predicate ordering="0">/Overall/PO_timedout/text()='false' and /Overall/POApproval/text()='true'</predicate>
          </flowsInto>
          <join code="and" />
          <split code="xor" />
          <startingMappings>
            <mapping>
              <expression query="&lt;PO_Manager&gt;{cm}&lt;/PO_Manager&gt;" />
              <mapsTo>PO_Manager</mapsTo>
            </mapping>
          </startingMappings>
          <completedMappings>
            <mapping>
              <expression query="&lt;PO_timedout&gt;{/Ordering/PO_timedout/text()}&lt;/PO_timedout&gt;" />
              <mapsTo>PO_timedout</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;POApproval&gt;{/Ordering/POApproval/text()}&lt;/POApproval&gt;" />
              <mapsTo>POApproval</mapsTo>
            </mapping>
          </completedMappings>
          <decomposesTo id="Ordering" />
        </task>
        <task id="Carrier_Appointment">
          <name>Carrier Appointment</name>
          <flowsInto>
            <nextElementRef id="Payment" />
            <predicate>/Overall/SP_timedout/text()='false'</predicate>
          </flowsInto>
          <flowsInto>
            <nextElementRef id="OutputCondition" />
            <predicate>/Overall/SP_timedout/text()='false'</predicate>
          </flowsInto>
          <flowsInto>
            <nextElementRef id="Freight_in_Transit" />
            <predicate>/Overall/SP_timedout/text()='false'</predicate>
            <isDefaultFlow />
          </flowsInto>
          <join code="and" />
          <split code="or" />
          <startingMappings>
            <mapping>
              <expression query="&lt;POrder&gt;{/Overall/POrder/*}&lt;/POrder&gt;" />
              <mapsTo>POrder</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;SP_timedout&gt;{false}&lt;/SP_timedout&gt;" />
              <mapsTo>SP_timedout</mapsTo>
            </mapping>
          </startingMappings>
          <completedMappings>
            <mapping>
              <expression query="&lt;TransportationQuote&gt;{/Carrier_Appointment/TransportationQuote/*}&lt;/TransportationQuote&gt;" />
              <mapsTo>TransportationQuote</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;SP_timedout&gt;{/Carrier_Appointment/SP_timedout/text()}&lt;/SP_timedout&gt;" />
              <mapsTo>SP_timedout</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;ShipmentNotice&gt;{/Carrier_Appointment/ShipmentNotice/*}&lt;/ShipmentNotice&gt;" />
              <mapsTo>ShipmentNotice</mapsTo>
            </mapping>
          </completedMappings>
          <decomposesTo id="Carrier_Appointment" />
        </task>
        <task id="Freight_in_Transit">
          <name>Freight in Transit</name>
          <flowsInto>
            <nextElementRef id="Freight_Delivered" />
            <isDefaultFlow />
          </flowsInto>
          <join code="and" />
          <split code="xor" />
          <startingMappings>
            <mapping>
              <expression query="&lt;POrder&gt;{/Overall/POrder/*}&lt;/POrder&gt;" />
              <mapsTo>POrder</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;TransportationQuote&gt;{/Overall/TransportationQuote/*}&lt;/TransportationQuote&gt;" />
              <mapsTo>TransportationQuote</mapsTo>
            </mapping>
          </startingMappings>
          <completedMappings>
            <mapping>
              <expression query="&lt;AcceptanceCertificate&gt;{/Freight_in_Transit/AcceptanceCertificate/*}&lt;/AcceptanceCertificate&gt;" />
              <mapsTo>AcceptanceCertificate</mapsTo>
            </mapping>
          </completedMappings>
          <decomposesTo id="Freight_in_Transit" />
        </task>
        <task id="Payment">
          <name>Payment</name>
          <flowsInto>
            <nextElementRef id="Freight_Delivered" />
            <isDefaultFlow />
          </flowsInto>
          <join code="and" />
          <split code="xor" />
          <startingMappings>
            <mapping>
              <expression query="&lt;POrder&gt;{/Overall/POrder/*}&lt;/POrder&gt;" />
              <mapsTo>POrder</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;TransportationQuote&gt;{/Overall/TransportationQuote/*}&lt;/TransportationQuote&gt;" />
              <mapsTo>TransportationQuote</mapsTo>
            </mapping>
          </startingMappings>
          <decomposesTo id="Payment" />
        </task>
        <task id="Freight_Delivered">
          <name>Freight Delivered</name>
          <flowsInto>
            <nextElementRef id="OutputCondition" />
            <isDefaultFlow />
          </flowsInto>
          <join code="and" />
          <split code="xor" />
          <startingMappings>
            <mapping>
              <expression query="&lt;AcceptanceCertificate&gt;{/Overall/AcceptanceCertificate/*}&lt;/AcceptanceCertificate&gt;" />
              <mapsTo>AcceptanceCertificate</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;POrder&gt;{/Overall/POrder/*}&lt;/POrder&gt;" />
              <mapsTo>POrder</mapsTo>
            </mapping>
            <mapping>
              <expression query="&lt;TransportationQuote&gt;{/Overall/TransportationQuote/*}&lt;/TransportationQuote&gt;" />
              <mapsTo>TransportationQuote</mapsTo>
            </mapping>
          </startingMappings>
          <decomposesTo id="Freight_Delivered" />
        </task>
        <outputCondition id="OutputCondition" />
      </processControlElements>
    </decomposition>
    <decomposition id="Carrier_Appointment" xsi:type="NetFactsType">
      <name>Carrier Appointment</name>
      <inputParam>
        <index>0</index>
        <name>POrder</name>
        <type>PurchaseOrderType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <inputParam>
        <index>1</index>
        <name>SP_timedout</name>
        <type>boolean</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </inputParam>
      <outputParam>
        <index>1</index>
        <name>SP_timedout</name>
        <type>boolean</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
      </outputParam>
      <outputParam>
        <index>2</index>
        <name>TransportationQuote</name>
        <type>TransportationQuoteType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <defaultValue />
      </outputParam>
      <outputParam>
        <index>12</index>
        <name>ShipmentNotice</name>
        <type>ShipmentNoticeType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <defaultValue />
      </outputParam>
      <localVariable>
        <index>0</index>
        <name>ShipmentNotice</name>
        <type>ShipmentNoticeType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>0</index>
        <name>TransportationQuote</name>
        <type>TransportationQuoteType</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>3</index>
        <name>Truckload</name>
        <type>boolean</type>
        <namespace>http://www.w3.org/2001/XMLSchema</namespace>
        <initialValue />
      </localVariable>
      <localVariable>
        <index>4</index>
        <name>DeliveryInstructions</name>
        <type>DeliveryInstructionsType</type>
